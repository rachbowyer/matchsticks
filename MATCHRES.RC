MatchMenu MENU 
BEGIN
	POPUP "&Game"
	BEGIN
		MENUITEM "&Start\tF2", GameStart
		MENUITEM "S&kill Level...", GameSkillLevel
		MENUITEM "&Resign", GameResign, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "E&xit", GameExit
	END

	POPUP "&Help"
	BEGIN
		MENUITEM "&About Matchsticks...", HelpAbout
	END

END

MatchAccelerator ACCELERATORS 
BEGIN
	VK_F2, GameStart, VIRTKEY
END

